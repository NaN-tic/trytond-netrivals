<tryton>
    <data>
        <!-- product netrivals -->
        <record model="ir.ui.view" id="product_netrivals_view_form">
            <field name="model">product.netrivals</field>
            <field name="type">form</field>
            <field name="name">product_netrivals_form</field>
        </record>
        <record model="ir.ui.view" id="product_netrivals_view_tree">
            <field name="model">product.netrivals</field>
            <field name="type">tree</field>
            <field name="name">product_netrivals_tree</field>
        </record>

        <!-- product.product -->
        <record model="ir.ui.view" id="product_view_form">
            <field name="model">product.product</field>
            <field name="type">form</field>
            <field name="inherit" ref="product.product_view_form"/>
            <field name="name">product_form</field>
        </record>
    </data>
</tryton>
